---
import Main from "@layouts/Main.astro";
import { Markdown } from 'astro/components';
const data = await fetch('https://api.storyblok.com/v1/cdn/stories' + `?&token=${import.meta.env.SB_TOKEN}&starts_with=politique-de-confidentialite&cv=${new Date}`)
const {stories} = await data.json()
---

<Main>
   <section class="markdown gap-4 max-w-[64ch] mx-auto">
    <Markdown content={stories[0].content.content}/>
   </section>
</Main>