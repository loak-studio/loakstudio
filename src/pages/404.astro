---
import Main from "../layouts/Main.astro"
import CustomButton from "../components/atoms/CustomButton.astro"
import mail from "../images/hero/mail-dynamic-clay.png"
import computer from "../images/hero/computer-dynamic-clay.png"
import figma from "../images/hero/figma-dynamic-clay.png"
import lock from "../images/hero/lock-dynamic-clay.png"
import bag from "../images/hero/bag-dynamic-clay.png"
---
<Main>
    <section class="px-4 md:px-0 relative min-h-[80vh] flex items-center justify-center mx-auto max-w-8xl mt-12">
        <img id="mail" class="md:block hidden absolute right-96 top-0 w-36 rotate-[27deg]" src={mail} alt="">
        <img id="computer" class="absolute w-52 -rotate-[14deg] left-7 top-12" src={computer} alt="">
        <img id="figma" class="absolute w-36 rotate-[35deg] right-12 top-40 blur-[2px]" src={figma} alt="">
        <img id="lock" class="absolute rotate-[40deg] w-36 left-0 bottom-10 blur-[3px]" src={lock} alt="">
        <img id="bag" class="md:block hidden absolute w-36 -rotate-[55deg] bottom-0 right-64" src={bag} alt="">
        <div class="relative z-10 flex flex-col items-center gap-10">
        <div>
        <svg class="w-full" width="426" height="278" viewBox="0 0 426 278" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
            <path d="M11.6477 182.17V167.682L75.5682 66.5455H86.0795V88.9886H78.9773L30.6818 165.409V166.545H116.761V182.17H11.6477ZM80.1136 212V177.767V171.02V66.5455H96.875V212H80.1136ZM308.718 182.17V167.682L372.638 66.5455H383.15V88.9886H376.048L327.752 165.409V166.545H413.832V182.17H308.718ZM377.184 212V177.767V171.02V66.5455H393.945V212H377.184Z" fill="black"/>
            <rect x="74" width="278" height="278" fill="url(#pattern0)"/>
            <defs>
            <pattern id="pattern0" patternContentUnits="objectBoundingBox" width="1" height="1">
            <use xlink:href="#image0_705_762" transform="scale(0.00438596)"/>
            </pattern>
            <image id="image0_705_762" width="228" height="228" xlink:href="data:image/png;base64,"/>
            </defs>
        </svg>

        </div>
            <h1 class="max-w-3xl text-5xl leading-tight text-center md:text-8xl">Oups !</h1>
            <p class="max-w-xl text-center text-lgrey-dark">La page que vous avez demandé n'existe pas..</p>
            <div class="flex flex-col gap-4 md:gap-10 md:flex-row">
                <CustomButton href="/">
                    Retourner à l'accueil
                </CustomButton>
            </div>
        </div>
    </section>
</Main>